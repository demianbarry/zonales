<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="persistancePU" transaction-type="RESOURCE_LOCAL">
    <provider>org.hibernate.ejb.HibernatePersistence</provider>
    <class>com.zonales.persistence.Eq</class>
    <class>com.zonales.persistence.ClaseAtributo</class>
    <class>com.zonales.persistence.ValorPermitidoAtrcomp</class>
    <class>com.zonales.persistence.ClaseComponente</class>
    <class>com.zonales.persistence.EqAtributos</class>
    <properties>
      <property name="hibernate.connection.username" value="services"/>
      <property name="hibernate.connection.driver_class" value="com.mysql.jdbc.Driver"/>
      <property name="hibernate.connection.password" value="services"/>
      <property name="hibernate.connection.url" value="jdbc:mysql://192.168.0.29:3306/services"/>
      <property name="hibernate.cache.provider_class" value="org.hibernate.cache.NoCacheProvider"/>
    </properties>
  </persistence-unit>
</persistence>
