<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.j2ee.ejbjarproject</type>
    <configuration>
        <buildExtensions xmlns="http://www.netbeans.org/ns/ant-build-extender/1">
            <extension file="jaxws-build.xml" id="jaxws"/>
            <extension file="xml_binding_build.xml" id="jaxb">
                <dependency dependsOn="jaxb-code-generation" target="-pre-pre-compile"/>
            </extension>
        </buildExtensions>
        <data xmlns="http://www.netbeans.org/ns/j2ee-ejbjarproject/3">
            <name>ZonalesSolrClient</name>
            <minimum-ant-version>1.6.5</minimum-ant-version>
            <included-library>file.reference.commons-codec-1.3.jar</included-library>
            <included-library>file.reference.commons-httpclient-3.1.jar</included-library>
            <included-library>file.reference.commons-io-1.3.1.jar</included-library>
            <included-library>file.reference.commons-logging-1.0.4.jar</included-library>
            <included-library>file.reference.apache-solr-common-1.3.0.jar</included-library>
            <included-library>file.reference.apache-solr-solrj-1.3.0.jar</included-library>
            <source-roots>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
    </configuration>
</project>
