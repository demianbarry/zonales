<?xml version="1.0" encoding="utf-8"?>
<install version="1.5" type="plugin" group="user">
    <name>User - Zonales Eq Creation</name>
    <author>Mediabit</author>
    <creationDate>Mayo 2010</creationDate>
    <copyright>Copyright (C) 2010 Mediabit. All rights reserved.</copyright>
    <license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>
    <authorEmail>admin@mediabit.com.ar</authorEmail>
    <authorUrl>www.mediabit.com.ar</authorUrl>
    <version>0.1</version>
    <description>Crea, elimina o modifica Ecualizadores según eventos en el Workflow de Usuarios (creación, modificación, etc)</description>
    <files>
        <filename plugin="eqevents">eqevents.php</filename>
    </files>
    <languages folder="language">
        <language tag="es-ES">es-ES.plg_user_eqevents.ini</language>
    </languages>
    <params>
        <param name="create_eq" type="radio" default="1" label="CREATE_EQ_IF_NOT_EXISTS" description="CREATE_EQ_IF_NOT_EXISTS_TIP">
            <option value="0">No</option>
            <option value="1">Yes</option>
        </param>
    </params>
</install>