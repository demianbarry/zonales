<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>ZCrawlSources</display-name>
    <servlet>
        <servlet-name>GetConfig</servlet-name>
        <servlet-class>org.zonales.crawlConfig.services.GetConfig</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SetConfig</servlet-name>
        <servlet-class>org.zonales.crawlConfig.services.SetConfig</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetConfig</servlet-name>
        <url-pattern>/getConfig</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SetConfig</servlet-name>
        <url-pattern>/setConfig</url-pattern>
    </servlet-mapping>
</web-app>
