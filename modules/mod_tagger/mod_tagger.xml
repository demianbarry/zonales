<?xml version="1.0" encoding="utf-8"?>
<install type="module" version="1.5.0">
    <name>Tagger Tree</name>
    <author>Cristian Pacheco</author>
    <creationDate>May 2010</creationDate>
    <copyright>(C) 2010 Cristian Pacheco. All rights reserved.</copyright>
    <license>http://www.gnu.org/copyleft/gpl.html GNU/GPL</license>
    <authorEmail>cpacheco@lavabit.com</authorEmail>
    <version>1.0.0</version>
    <description>Tagger Tree permite agregar bandas al ecualizador
    </description>
    <files>

        <filename>index.html</filename>
        <filename module="mod_tagger">mod_tagger.php</filename>
        <filename>tmpl/default.php</filename>
        <filename>tmpl/index.html</filename>

    </files>
    <params>
        <param name="register_url" type="text" label="URL de la pantalla de registro"
               default="index.php?option=com_user&amp;view=register" description="Ingrese la URL interna a la pantalla de registro de usuarios" />
    </params>
    <languages folder="language">
        <language tag="es-ES">es-ES.mod_tagger.ini</language>
    </languages>
</install>
