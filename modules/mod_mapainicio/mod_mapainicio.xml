<?xml version="1.0" encoding="utf-8"?>
<install type="module" version="1.5.0">
  <name>Mapa Zonales Inicio</name>
  <author>Mediabit</author>
  <creationDate>Mayo 2009</creationDate>
  <copyright/>
  <license/>
  <authorEmail/>
  <authorUrl>http://www.mediabit.com.ar</authorUrl>
  <version>0.1</version>
  <description>MOD_MAPAINICIO_DESC</description>
  <files>
    <filename module="mod_mapainicio">mod_mapainicio.php</filename>
    <filename>index.html</filename>
    <filename>tmpl/default.php</filename>
    <filename>tmpl/index.html</filename>
  </files>
  <languages folder="language">
    <language tag="es-ES">es-ES.mod_mapainicio.ini</language>
  </languages>
  <params>
    <param name="width" type="text" default="800" label="WIDTH" description="TIP_WIDTH"/>
    <param name="height" type="text" default="600" label="HEIGHT" description="TIP_HEIGHT"/>
    <param name="wmode" type="radio" default="0" label="Transparente" description="TIP_WMODE">
      <option value="0">NO</option>
      <option value="1">YES</option>
    </param>
  </params>
  <params group="advanced">
    <param name="cache" type="radio" default="0" label="CACHING" description="SELECT WHETHER TO CACHE THE CONTENT OF THIS MODULE" >
      <option value="0">NO</option>
      <option value="1">YES</option>
    </param>
  </params>
</install>

